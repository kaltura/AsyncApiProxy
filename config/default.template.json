{
  "logger": {
    "debugEnabled": 0,
    "logDir": "@LOG_DIR@",
    "logName": "kaltura_async_proxy.log",
    "accessLogName": "kaltura_async_proxy_access.log",
    "errorLogName": "kaltura_async_proxy_error.log"
  },

  "server": {
    "version": "v1.2.0",
    "remote_addr_header_salt": "@APP_REMOTE_ADDR_HEADER_SALT@",
    "remote_addr_header_timeout": 120
  },

  "cdn_api_host": {
    "cdn_api_host_http": "@WWW_HOST@/api_v3/",
    "cdn_api_host_https": "@WWW_HOST@/api_v3/"
  },

  "cloud": {
    "requestTimeout": 100000,
    "httpPort1": "@HTTP_PORT1@"
    //"httpPort2": "@HTTP_PORT2@",
    "httpsPort1": "@HTTPS_PORT1@"
//    "httpsPort2": "@HTTPS_PORT2@",
  },

  "throttle": {
    //example - 10 messages per second will be [messagePerTimePeriod:10 , timePeriod: 1000]
    "messagePerTimePeriod": 10,
    "timePeriod": 1000,
    "httpsHeader": "@HTTPS_HEADER@"
  },

  "handlers": [
    //history example
    {
      "identifier": {
        "service": "userentry",
        "action": "update"
      },
      "cacheKeyParams": ["id"],
      //ttl in milliseconds
      "ttl": 30000,
      "extendTtl": true
    },

    {
      "identifier": {
        "service": "cuepoint_cuepoint",
        "action": "update"
      },
      "cacheKeyParams": ["id"],
      //ttl in milliseconds
      "ttl": 2000,
      "extendTtl": false
    },

    {
      "identifier": {
        "service": "servernode",
        "action": "reportStatus"
      },
      "cacheKeyParams": ["hostName"],
      //ttl in milliseconds
      "ttl": 10000,
      "extendTtl": true,
      "preCacheSendSetting": {
        "sendBeforeCaching" : true,
        "sendExtraParams" : false
      },
      "requestExtraParams": {
        "status": "4"
      }
    }
  ]
}
